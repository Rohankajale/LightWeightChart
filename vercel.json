  {
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "name": "Chartingo",
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/main.py"
      }
    ],
    "installCommand": "pip install -r requirements.txt"
  }
  
